<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="CODA LACEK" allow-return-values="true" verbose="1"
	parallel="tests" thread-count="3">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>
	<test name="REGIONS ADD TESTCASES" group-by-instances="true">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="layout.Regions_TestCases" />
		</classes>
	</test>
	<test name="REGIONS EDIT TESTCASES" group-by-instances="true">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="layout.RegionsEdit_testcases" />
		</classes>
	</test>
	<test name="REGIONS RESTORE TESTCASES" group-by-instances="true">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="layout.RegionsRestore_testcases" />
		</classes>
	</test>
</suite> 
