<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="CODA LACEK" allow-return-values="true" verbose="1"
	parallel="tests" thread-count="3">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>
	<test name="SERIES ADD TESTCASES" group-by-instances="true">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="library.Series_Testcases" />
		</classes>
	</test>
	<test name="SERIES EDIT TESTCASES" group-by-instances="true">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="library.SeriesEdit_Testcases" />
		</classes>
	</test>
	<test name="SERIES RESTORE TESTCASES" group-by-instances="true">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="library.SeriesRestore_Testcases" />
		</classes>
	</test>
	<test name="SERIES PUBLISH TESTCASES" group-by-instances="true">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="library.SeriesPublish_Testcases" />
		</classes>
	</test>
</suite> 

