<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="CODA LACEK" allow-return-values="true" verbose="1"
	parallel="tests" thread-count="4">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>
	<test name="TRACKS ADD TESTCASES" group-by-instances="true">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="library.Tracks_Testcases" />
		</classes>
	</test>
	<test name="TRACKS EDIT TESTCASES" group-by-instances="true">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="library.TracksEdit_Testcases" />
		</classes>
	</test>
	<test name="TRACKS RESTORE TESTCASES" group-by-instances="true">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="library.TracksRestore_Testcases" />
		</classes>
	</test>
	<test name="TRACKS PUBLISH TESTCASES" group-by-instances="true">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="library.TracksPublish_Testcases" />
		</classes>
	</test>
</suite> 