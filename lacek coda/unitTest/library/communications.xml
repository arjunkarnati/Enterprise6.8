<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="CODA LACEK" allow-return-values="true" verbose="1"
	parallel="tests" thread-count="4">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>
	<test name="COMMUNICATIONS ADD TESTCASES" group-by-instances="true">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="library.Communications_TestCases" />
		</classes>
	</test>
	<test name="COMMUNICATIONS EDIT TESTCASES" group-by-instances="true">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="library.CommunicationsEdit_Testcases" />
		</classes>
	</test>
	<test name="COMMUNICATIONS RESTORE TESTCASES" group-by-instances="true">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="library.CommunicationsRestore_Testcases" />
		</classes>
	</test>
	<test name="COMMUNICATIONS PUBLISH TESTCASES" group-by-instances="true">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="library.CommunicationsPublish_Testcases" />
		</classes>
	</test>
</suite>  

